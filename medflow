src/main.jsx
import React from "react";
import ReactDOM from "react-dom/client";
import { BrowserRouter } from "react-router-dom";
import App from "./App";
import "./index.css";

ReactDOM.createRoot(document.getElementById("root")).render(
  <BrowserRouter>
    <App />
  </BrowserRouter>
);
src/App.jsx
import { Routes, Route } from "react-router-dom";
import Sidebar from "./components/Sidebar";
import Header from "./components/Header";
import AcademicPlanner from "./pages/AcademicPlanner";
import StudyPlanner from "./pages/StudyPlanner";
import ClinicalPlanner from "./pages/ClinicalPlanner";
import ExamPlanner from "./pages/ExamPlanner";

export default function App() {
  return (
    <div className="flex h-screen">
      <Sidebar />
      <div className="flex flex-col flex-1">
        <Header />
        <main className="p-6 overflow-y-auto flex-1">
          <Routes>
            <Route path="/" element={<AcademicPlanner />} />
            <Route path="/study" element={<StudyPlanner />} />
            <Route path="/clinical" element={<ClinicalPlanner />} />
            <Route path="/exam" element={<ExamPlanner />} />
          </Routes>
        </main>
      </div>
    </div>
  );
}
src/components/Sidebar.jsx
import { Link } from "react-router-dom";

export default function Sidebar() {
  return (
    <aside className="w-64 bg-blue-900 text-white flex flex-col">
      <div className="p-4 text-2xl font-bold border-b border-blue-700">
        MedFlow
      </div>
      <nav className="flex-1 p-4 space-y-2">
        <Link to="/" className="block p-2 rounded hover:bg-blue-700">
          Academic Planner
        </Link>
        <Link to="/study" className="block p-2 rounded hover:bg-blue-700">
          Study Planner
        </Link>
        <Link to="/clinical" className="block p-2 rounded hover:bg-blue-700">
          Clinical Planner
        </Link>
        <Link to="/exam" className="block p-2 rounded hover:bg-blue-700">
          Exam Planner
        </Link>
      </nav>
    </aside>
  );
}
